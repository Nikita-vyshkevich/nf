    "164166498934ac4d9c": {
      "type": "banner-flexy-col",
      "blocks": {
        "16416649899182c42b-0": {
          "type": "heading",
          "settings": {
            "heading": "From Seed to Shelf",
            "heading_vertical_line_height": 10,
            "heading_vertical_line_margin_left": 50
          }
        },
        "16416649899182c42b-4": {
          "type": "empty_block",
